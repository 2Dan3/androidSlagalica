<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".TabbedMainFragment">

    <com.google.android.material.tabs.TabLayout
        android:id="@+id/tablayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/side_nav_bar"
        app:tabTextColor="@color/orange_light"
        app:tabIndicatorColor="@color/orange_light"
        app:tabSelectedTextColor="@color/orange_dark"
        app:tabIconTint="@color/tab_icon_selector">
        <com.google.android.material.tabs.TabItem
            android:id="@+id/ranktab"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />
        <com.google.android.material.tabs.TabItem
            android:id="@+id/maintab"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />
        <com.google.android.material.tabs.TabItem
            android:id="@+id/playerstab"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />
    </com.google.android.material.tabs.TabLayout>

</FrameLayout>