                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:textColor="@color/blue_dark"
                    android:textStyle="bold"
                    android:textSize="14dp"/>
            </androidx.cardview.widget.CardView>


