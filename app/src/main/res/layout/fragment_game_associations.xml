            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardBackgroundColor="@color/white_smoked"
                android:layout_marginTop="10dp"
                >
                <TextView
